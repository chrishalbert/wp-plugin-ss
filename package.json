{
  "name": "wp-plugin-ss",
  "version": "0.2.0",
  "scripts": {
    "build": "npm run-script lint && npm run-script test-cov",
    "test": "./node_modules/mocha/bin/mocha --timeout 5000 --check-leaks test/ --recursive",
    "test-cov": "./node_modules/istanbul/lib/cli.js cover --include-all-sources true node_modules/mocha/bin/_mocha -- --reporter dot --timeout 5000 --check-leaks test/ --recursive",
    "lint": "./node_modules/eslint/bin/eslint.js **/*.js",
    "start": "node --max-old-space_size=512 ./bin/www"
  },
  "keywords": [
    "wordpress-plugin",
    "nodejs",
    "api",
    "wordpress-plugin-search"
  ],
  "bin": {
    "wp-plugin-ss": "bin/www"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "~1.13.2",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-validator": "^3.2.0",
    "jade": "~1.11.0",
    "mongodb": "^2.2.30",
    "morgan": "~1.6.1",
    "redis": "^2.7.1",
    "serve-favicon": "~2.3.0",
    "swagger-node-express": "^2.1.3",
    "swagger-ui-express": "^2.0.1",
    "thenby": "^1.2.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsdoc": "^0.3.2",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsdoc": "^3.1.1",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mock-require": "^2.0.2",
    "mongo-mock": "^2.4.0",
    "nock": "^9.0.13",
    "should": "^11.2.1",
    "supertest": "^3.0.0"
  }
}
